{
"manifest_version": 2,
    "name": "Capitalize my Name",
    "description": "Makes any instance of my name all capital letters",
    "version": "1.0",
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}
